		getCharBasedOnLangId=function(arrayObj){
			var chars = arrayObj[langID];
			var langArr = new Array();
			var character;
			
			if(chars == null){
				chars = arrayObj["001"];
			} 
			var charLen = chars.length;
			for(i=0; i < charLen ; i++){
				character = chars.substring(i,i+1);
				if(character == "\""){
					langArr[i] = "&quot";
				}else{
					langArr[i] = character;	
				}
			}		
			return langArr;
		};
		
		
		settingPinPadCtlOnFocus=function(whatobject,whatimage,whatelement)
		{ 
			//alert("hi");
			
			
			<!-- Added By Anik -->
			//alert(document.getElementById("AuthenticationFG.ACCESS_CODE_keyboard")+"hiiiiiii");
			if(document.getElementById('Disable_Virtual_keyBoard').checked==true)
