var urlString=window.location.href;
var getUrlParameter = function getUrlParameter(sParam) {
    var sPageURL = window.location.search.substring(1),
        sURLVariables = sPageURL.split('&'),
        sParameterName,
        i;

    for (i = 0; i < sURLVariables.length; i++) {
        sParameterName = sURLVariables[i].split('=');

        if (sParameterName[0] === sParam) {
            return sParameterName[1] === undefined ? true : decodeURIComponent(sParameterName[1]);
        }
    }
};
var bankID=getUrlParameter("BANK_ID");
var langID=getUrlParameter("LANGUAGE_ID");

if(bankID){
	document.cookie = "langId=" + langID;
}

if((bankID== 'ML' || bankID== 'BF' || bankID== 'BJ' || bankID== 'SN' || bankID== 'GN' || bankID== 'CI') && langID == null){
	window.location.href= urlString + "&LANGUAGE_ID=002";
		document.cookie = "langId=002";	
	
	
}

