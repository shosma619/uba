				}
			else
				{
				//alert("inside else");
				var element=document.getElementById("AuthenticationFG.ACCESS_CODE_keyboard");
			element.style.display='block';
			//alert(document.getElementById("AuthenticationFG.ACCESS_CODE_keyboard"));
